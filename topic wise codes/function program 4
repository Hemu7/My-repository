import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        System.out.println("enter the two numbers");
        Scanner in=new Scanner(System.in);
        int a=in.nextInt();
        int b=in.nextInt();
        int sum= add(a,b);
        System.out.println(sum);
    }

    static int add(int x, int y) {
        int ex=x+y;
        return ex;
    }


}
